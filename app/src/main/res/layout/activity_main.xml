<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="register" android:id="@+id/register">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="have one account?"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.349"
            app:layout_constraintVertical_bias="0.964"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="sign in"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.612"
            app:layout_constraintVertical_bias="0.964" android:id="@+id/textView"
            android:textColor="@android:color/background_dark"/>
    <EditText
            android:layout_width="267dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="full name"
            android:ems="10"
            android:id="@+id/editText"
            android:textColor="@android:color/darker_gray" android:drawableLeft="@drawable/D"
            android:drawableRight="@drawable/4" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginTop="140dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/editText3" app:layout_constraintVertical_bias="0.76"/>
    <EditText
            android:layout_width="267dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:text="password"
            android:ems="10"
            android:id="@+id/editText4"
            android:textColor="@android:color/darker_gray" android:drawableRight="@drawable/B"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/editText5" android:drawableLeft="@drawable/C"/>
    <EditText
            android:layout_width="267dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Cell phone"
            android:ems="10"
            android:id="@+id/editText5"
            android:textColor="@android:color/darker_gray" android:drawableLeft="@drawable/E"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/editText6"/>
    <EditText
            android:layout_width="267dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Country"
            android:ems="10"
            tools:layout_editor_absoluteY="405dp" tools:layout_editor_absoluteX="72dp" android:id="@+id/editText6"
            android:textColor="@android:color/darker_gray"/>
    <EditText
            android:layout_width="267dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="FULL NAME"
            android:ems="10"
            android:id="@+id/editText3"
            android:textColor="@android:color/darker_gray" android:drawableLeft="@drawable/D"
            android:drawableRight="@drawable/4" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginBottom="20dp" app:layout_constraintBottom_toTopOf="@+id/editText4"/>
    <Button
            android:text="Agree & Create Account"
            android:layout_width="267dp"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="481dp"
            tools:layout_editor_absoluteX="72dp" android:id="@+id/button"
            android:textColor="@android:color/background_light" android:background="@android:color/holo_red_dark"/>

</androidx.constraintlayout.widget.ConstraintLayout>